{
    "name": "SABconnect",
    "version": "0.2",
    "description": "SABnzbd extension for Google Chrome.",
    "background_page": "background.html",
    "toolstrips": [
    "sab_toolstrip.html"
    ],
    "content_scripts": [
        {
            "matches": [
                "http://*.newzbin.com/*",
                "https://*.newzbin.com/*",
                "http://*.newzxxx.com/*",
                "https://*.newzxxx.com/*"
            ],
            "js": ["jquery-1.3.2.min.js", "common.js", "newzbin.js"]
        },
        {
            "matches": [
                "http://*.tvnzb.com/",
                "http://*.tvnzb.com/*",
                "https://*.tvnzb.com/",
                "https://*.tvnzb.com/*"
            ],
            "js": ["jquery-1.3.2.min.js", "common.js", "newzbin.js"]
        },
        {
            "matches": [
                "http://*.nzbmatrix.com/",
                "http://*.nzbmatrix.com/*",
                "https://*.nzbmatrix.com/",
                "https://*.nzbmatrix.com/*",
                "http://*.nzbxxx.com/",
                "http://*.nzbxxx.com/*",
                "https://*.nzbxxx.com/",
                "https://*.nzbxxx.com/*"
            ],
            "js": ["jquery-1.3.2.min.js", "common.js", "nzbmatrix.js"]
        }
    ],
    "icons": { "16": "images/sab2_16.png",
               "32": "images/sab2_32.png",
              "128": "images/sab2_128.png" },

    "permissions": [
        "http://*/*",
        "https://*/*",
        "tabs"
    ]
}